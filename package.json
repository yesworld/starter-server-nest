{
  "name": "starter-server-nest",
  "version": "0.0.6",
  "description": "It's a starter framework on Nest, with TypeScript support. Middleware JWT.",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/main.js",
    "start:dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node -r tsconfig-paths/register ./src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "sync": "ts-node ./node_modules/.bin/typeorm schema:sync",
    "format": "prettier --write \"src/**/*.ts\"",
    "prestart:prod": "rimraf dist && yarn run build",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^6.0.0",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/typeorm": "^6.0.0",
    "bcrypt": "^3.0.5",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dotenv": "^7.0.0",
    "express-rate-limit": "^3.4.0",
    "helmet": "^3.16.0",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "typeorm": "^0.2.15"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^24.0.11",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^11.11.5",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.7",
    "jest": "^24.5.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.14.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "typescript": "^3.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "main": "src/main.ts",
  "author": {
    "name": "yesworld",
    "url": "https://github.com/yesworld"
  },
  "license": "MIT",
  "keywords": [
    "typescript",
    "koa",
    "koa2",
    "jwt",
    "rest",
    "starter",
    "typeORM",
    "class-validator",
    "routing-controllers",
    "sql",
    "api",
    "DI",
    "typedi",
    "cors"
  ]
}
